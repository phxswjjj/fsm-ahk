

OutputLog(msg)
{
    timeStr := FormatTime(, "hh:mm:ss")
    OutputDebug("[" timeStr "] " msg "`n")
}

Between(var, min, max)
{
    if var>=min  and var<=max
        return true
    else
        return false
}

ArrayContain(array, target) {
    for (item in array) {
        if (item = target)
            return true
    }
    return false
}